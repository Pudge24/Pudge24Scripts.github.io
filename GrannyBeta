local Toggle = ExpTab:CreateToggle({
   Name = "Loop Delete Granny",
   CurrentValue = false,
   Flag = "toggle1",
   Callback = function(CurrentValue)
      if CurrentValue then
         -- Start the loop
         _G.loop1 = true
         while _G.loop1 do
            local function removeGranny()
               for i = 1, 5 do
                   local preset = workspace:FindFirstChild("Preset" .. i)
                   if preset then
                       local granny = preset.Locks:FindFirstChild("Granny")
                       if granny then
                           granny:Destroy()
                       end
                   end
               end
           end
           
           while true do
               removeGranny()
               wait(20) -- –ñ–¥–µ–º 20 —Å–µ–∫—É–Ω–¥ –ø–µ—Ä–µ–¥ —Å–ª–µ–¥—É—é—â–∏–º —É–¥–∞–ª–µ–Ω–∏–µ–º
           end           
         end
      else
         -- Stop the loop
         _G.loop1 = false
      end
   end,
})



