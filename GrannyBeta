      if CurrentValue then
         -- Start the loop
         _G.loop4 = true
         while _G.loop4 do
            game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 55
            wait(0.1) -- Adjust the wait time as needed
         end
      else
         -- Stop the loop
         _G.loop4 = false
      end
   end,
})


